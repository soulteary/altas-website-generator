{
  "name": "altas-website-generator",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development node webpack/dev.js",
    "build": "NODE_ENV=production node webpack/prod.js",
    "lint": "node_modules/.bin/eslint src/scripts",
    "test": "jest --coverage --coverageDirectory",
    "gitbook":"node_modules/.bin/gitbook atlas-docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soulteary/altas-website-generator.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/soulteary/altas-website-generator/issues"
  },
  "hexo": {
    "version": "3.2.2"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "dependencies": {
    "cheerio": "^0.22.0",
    "hexo": "^3.2.0",
    "hexo-deployer-git": "^0.2.0",
    "hexo-extend-plugin": "^0.1.10",
    "hexo-generator-index": "^0.2.0",
    "hexo-html-minifier": "0.0.1",
    "hexo-renderer-ejs": "^0.2.0",
    "hexo-renderer-marked": "^0.2.11",
    "hexo-server": "^0.2.0"
  },
  "hexoOrigin": {
    "version": "3.2.2"
  },
  "hexoExtendFeature": {
    "source_posts_base_dir": [
      "./posts/"
    ],
    "enable_components_demo_raw_content": [
      "on",
      "components的demo目录保持内容为raw"
    ],
    "extend_is_helper": [
      "on",
      "使用增强的is辅助函数"
    ],
    "components_list": [
      "off",
      "使用组件列表侧边栏"
    ],
    "cancel_escape_at_document_title": [
      "off",
      "组件文档页面标题不转义多次"
    ],
    "archive_dirs_structure_in_source_dir": [
      "on",
      "源文件目录使用年月日方式保持目录结构"
    ],
    "remove_date_prefix_in_post_url": [
      "on",
      "去掉文档页面URL中存在的日期字符串"
    ]
  },
  "homepage": "https://github.com/soulteary/altas-website-generator#readme",
  "devDependencies": {
    "autoprefixer": "^6.7.5",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-runtime": "^6.23.0",
    "clean-webpack-plugin": "^0.1.15",
    "connect-history-api-fallback": "^1.1.0",
    "copy-webpack-plugin": "^3.0.1",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.2",
    "eslint": "^3.16.1",
    "eslint-friendly-formatter": "^2.0.7",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-import": "^2.2.0",
    "express": "^4.13.3",
    "express-favicon": "^1.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.9.0",
    "gitbook-cli": "^2.3.0",
    "glob": "^7.1.1",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.28.0",
    "imports-loader": "^0.7.1",
    "inject-loader": "^2.0.1",
    "jest": "^19.0.2",
    "json-loader": "^0.5.4",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "mock2easy": "0.0.20",
    "open-browser-webpack-express-plugin": "^0.0.3",
    "open-browser-webpack-plugin": "^0.0.4",
    "postcss-loader": "^1.3.2",
    "precss": "^1.4.0",
    "regenerator-runtime": "^0.10.3",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.8",
    "watch": "^1.0.2",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1",
    "webpack-vendor-chunk-plugin": "^1.0.0"
  }
}
